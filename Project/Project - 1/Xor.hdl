/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=a,out=a1);
    Not(in=b,out=b1);
    And(a=a1,b=b,out=out1);
    And(a=a,b=b1,out=out2);
    Or(a=out1,b=out2,out=out);   
}

